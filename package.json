{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "commercequest-space",
  "title": "CommerceQuest.space",
  "description": "Access Spryker's CommerceQuest forum",
  "icon": "command-icon.png",
  "author": "skroll",
  "categories": [
    "Developer Tools",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "CommerceQuest Search",
      "subtitle": "Search the forum",
      "description": "Search Spryker's CommerceQuest forum",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "numSearchResults",
      "type": "dropdown",
      "required": false,
      "title": "Searchresults",
      "description": "Number of searchresults to fetch",
      "default": "10",
      "data": [
        {
          "title": "10",
          "value": "10"
        },
        {
          "title": "25",
          "value": "25"
        },
        {
          "title": "50",
          "value": "50"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
